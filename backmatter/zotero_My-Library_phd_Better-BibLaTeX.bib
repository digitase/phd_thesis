  file = {/Users/bb9/Zotero/storage/ME9344J9/Nakaya et al_2011_Nature Immunology_Systems biology of vaccination for seasonal influenza in humans_SM.pdf;/Users/bb9/Zotero/storage/Q42L6WQX/Nakaya et al_2011_Nature Immunology_Systems biology of vaccination for seasonal influenza in humans.pdf},
  isbn = {1529-2916 (Electronic) 1529-2908 (Linking)},
  keywords = {influenza,influenza; seasonal,predictive modelling,systems vaccinology,vaccine},
  number = {8}
}

