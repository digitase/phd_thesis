  author = {Li, Xue and Meng, Xiangrui and Spiliopoulou, Athina and Timofeeva, Maria and Wei, Wei-Qi and Gifford, Aliya and Shen, Xia and He, Yazhou and Varley, Tim and McKeigue, Paul and Tzoulaki, Ioanna and Wright, Alan F and Joshi, Peter and Denny, Joshua C and Campbell, Harry and Theodoratou, Evropi},
