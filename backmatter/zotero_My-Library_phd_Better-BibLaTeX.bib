@article{brandt2020AutoimmuneDiseaseRisk,
  title = {An Autoimmune Disease Risk Variant Has a Trans Master Regulatory Effect Mediated by {{IRF1}} under Immune Stimulation},
  author = {Brandt, Margot K and Kim-Hellmuth, Sarah and Ziosi, Marcello and Gokden, Alper and Wolman, Aaron and Lam, Nora and Recinos, Yocelyn and Hornung, Veit K and Schumacher, Johannes and Lappalainen, Tuuli},
