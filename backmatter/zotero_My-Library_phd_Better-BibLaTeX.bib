  author = {Shu, Le and Zhao, Yuqi and Kurt, Zeyneb and Byars, Sean Geoffrey and Tukiainen, Taru and Kettunen, Johannes and Orozco, Luz D. and Pellegrini, Matteo and Lusis, Aldons J. and Ripatti, Samuli and Zhang, Bin and Inouye, Michael and Mäkinen, Ville-Petteri and Yang, Xia},
  date = {2016-12-04},
