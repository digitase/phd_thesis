  number = {6}
}

@article{duong2017ApplyingMetaanalysisGenotypetissue,
  title = {Applying Meta-Analysis to Genotype-Tissue Expression Data from Multiple Tissues to Identify {{eQTLs}} and Increase the Number of {{eGenes}}},
  author = {Duong, Dat and Gai, Lisa and Snir, Sagi and Kang, Eun Yong and Han, Buhm and Sul, Jae Hoon and Eskin, Eleazar},
