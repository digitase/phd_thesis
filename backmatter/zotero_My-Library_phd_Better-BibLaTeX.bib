  pages = {e1003486},
  keywords = {methods,quantitative trait loci; expression (eQTLs),_unread_med_priority,Bayesian model averaging (BMA),Bayesian inference,genetic association,quantitative trait loci (QTL),eQtlBma,multivariate/joint model},
  author = {Flutre, Timothée and Wen, Xiaoquan and Pritchard, Jonathan and Stephens, Matthew},
  file = {/Users/bb9/Zotero/storage/T2W7ZDJ7/Flutre et al_2013_PLOS Genet_A Statistical Framework for Joint eQTL Analysis in Multiple Tissues.pdf;/Users/bb9/Zotero/storage/GQ9WUCMW/article.html}
