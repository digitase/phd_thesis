  eprinttype = {pmid},
  file = {/Users/bb9/Zotero/storage/D6W9879M/Ritchie et al_2015_Nucleic Acids Research_limma powers differential expression analyses for RNA-sequencing and microarray.pdf;/Users/bb9/Zotero/storage/NHZUD6D9/e47.html},
