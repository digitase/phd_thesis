  eprinttype = {pmid},
  file = {/Users/bb9/Zotero/storage/CY3EZGNS/Kim et al_2014_Translational Psychiatry_A meta-analysis of gene expression quantitative trait loci in brain.pdf;/Users/bb9/Zotero/storage/EMREUPNY/Kim et al_2014_Translational Psychiatry_A meta-analysis of gene expression quantitative trait loci in brain_(SM).pdf},
  isbn = {2158-3188 (Electronic)\textbackslash r2158-3188 (Linking)},
  keywords = {_unread_med_priority,meta-analysis,meta-analysis; fixed-effect (not fixed-effects),quantitative trait loci (QTL),quantitative trait loci; expression (eQTLs)},
