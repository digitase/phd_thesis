  urldate = {2020-02-25},
  abstract = {Functional mechanisms remain unknown for most genetic loci associated to complex human traits and diseases. In this study, we first mapped trans-eQTLs in a data set of primary monocytes stimulated with LPS, and discovered that a risk variant for autoimmune disease, rs17622517 in an intron of C5ORF56, affects the expression of the transcription factor IRF1 20 kb away. The cis-regulatory effect on IRF1 is active under early immune stimulus, with a large number of trans-eQTL effects across the genome under late LPS response. Using CRISPRi silencing, we showed that the SNP locus indeed functions as an IRF1 enhancer with widespread transcriptional effects. Genome editing by CRISPR further indicated that rs17622517 is indeed a causal variant in this locus, and recapitulated the LPS-specific trans-eQTL signal. Our results suggest that this common genetic variant affects inter-individual response to immune stimuli via regulation of IRF1. For this autoimmune GWAS locus, our work provides evidence of the causal variant, demonstrates a condition-specific enhancer effect, identifies IRF1 as the likely causal gene in cis, and indicates that overactivation of the downstream immune-related pathway may be the cellular mechanism increasing disease risk. This work not only provides rare experimental validation of a master-regulatory trans-eQTL, but also demonstrates the power of eQTL mapping to build mechanistic hypotheses amenable for experimental follow-up using the CRISPR toolkit.},
  file = {/Users/bb9/Zotero/storage/7VGU7ZYP/Brandt et al_2020_bioRxiv_An autoimmune disease risk variant has a trans master regulatory effect.pdf},
  keywords = {_untagged_working_set,genotype-environment interaction (GxE),quantitative trait loci (QTL),quantitative trait loci; trans},
  langid = {english}
}

@article{breitling2004RankProductsSimple,
  title = {Rank Products: A Simple, yet Powerful, New Method to Detect Differentially Regulated Genes in Replicated Microarray Experiments},
  shorttitle = {Rank Products},
  author = {Breitling, Rainer and Armengaud, Patrick and Amtmann, Anna and Herzyk, Pawel},
  date = {2004-08-27},
  journaltitle = {FEBS Letters},
  volume = {573},
  pages = {83--92},
  issn = {00145793},
  doi = {10.1016/j.febslet.2004.07.055},
  url = {http://doi.wiley.com/10.1016/j.febslet.2004.07.055},
  urldate = {2018-07-12},
  abstract = {One of the main objectives in the analysis of microarray experiments is the identification of genes that are differentially expressed under two experimental conditions. This task is complicated by the noisiness of the data and the large number of genes that are examined simultaneously. Here, we present a novel technique for identifying differentially expressed genes that does not originate from a sophisticated statistical model but rather from an analysis of biological reasoning. The new technique, which is based on calculating rank products (RP) from replicate experiments, is fast and simple. At the same time, it provides a straightforward and statistically stringent way to determine the significance level for each gene and allows for the flexible control of the false-detection rate and familywise error rate in the multiple testing situation of a microarray experiment. We use the RP technique on three biological data sets and show that in each case it performs more reliably and consistently than the non-parametric t-test variant implemented in Tusher et al.’s significance analysis of microarrays (SAM). We also show that the RP results are reliable in highly noisy data. An analysis of the physiological function of the identified genes indicates that the RP approach is powerful for identifying biologically relevant expression changes. In addition, using RP can lead to a sharp reduction in the number of replicate experiments needed to obtain reproducible results.},
  file = {/Users/bb9/Zotero/storage/37DWZLG8/Breitling et al_2004_FEBS Letters_Rank products.pdf},
  keywords = {methods,Rank Product (RP),transcriptomics,transcriptomics; differential gene expression},
  langid = {english},
  number = {1-3}
}

